# Server Configuration
PORT=3006
NODE_ENV=production
FRONTEND_URL=https://tsindeka.merdekabattery.com

# Database Configuration
DB_TYPE=mssql
DB_HOST=your-db-host
DB_SERVER=your-db-server
DB_DATABASE=your-database-name
DB_USER=your-db-user
DB_PASSWORD=your-db-password
DB_PORT=1433
DB_ENCRYPT=false
DB_TRUST_CERT=true

# N8N Configuration
#N8N_WEBHOOK_URL=https://your-n8n-domain.com:5679/webhook-test/
N8N_WEBHOOK_URL=https://your-n8n-domain.com:5679/webhook/

# Webhook Timeout Configuration (in milliseconds)
CHAT_WEBHOOK_TIMEOUT=120000
TRAINING_WEBHOOK_TIMEOUT=120000
PROCESSING_WEBHOOK_TIMEOUT=120000
# Security
JWT_SECRET=your-jwt-secret-key
API_KEY=your-api-key-here
WEBHOOK_SECRET=your-webhook-secret-here

# Admin Configuration
ADMIN_USERNAME=admin
ADMIN_PASSWORD=your-admin-password-here

# Application Configuration
VITE_APP_NAME=AI Insight
VITE_APP_VERSION=1.0.0

# Logging
LOG_LEVEL=info

# LDAP Configuration (Active Directory)
LDAP_URL=ldap://your-ldap-server:389
LDAP_BASE_DN=dc=your-domain,dc=com
LDAP_USERNAME=your-domain\your-ldap-user
LDAP_PASSWORD="your-ldap-password"
BIND_DN=CN=Your LDAP User,OU=Your OU,DC=your-domain,DC=com
BIND_PW="your-bind-password"
BASE_OU=DC=your-domain,DC=com

SFTP_HOST=your-sftp-host
SFTP_USERNAME=your-sftp-username
SFTP_PASSWORD=your-sftp-password
SFTP_PORT=22
SFTP_UPLOAD_PATH=/your-upload-path
