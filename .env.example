# Frontend Environment Variables

# API Configuration
VITE_API_BASE_URL=http://localhost:3001/api

# N8N Configuration (Optional)
# Primary webhook URL for N8N integration (base URL - paths will be auto-appended)
VITE_N8N_WEBHOOK_URL=https://your-n8n-server.com:5679/webhook-test/

# N8N API Key (if required for authentication)
VITE_N8N_API_KEY=

# Development Configuration
VITE_DEV_MODE=true

# Typewriter Animation Configuration
# Enable or disable typewriter animation (true/false)
VITE_TYPEWRITER_ENABLED=true
# Speed of typewriter animation in milliseconds per character
VITE_TYPEWRITER_SPEED=30